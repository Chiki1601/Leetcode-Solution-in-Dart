class Solution {
  int minOperations(List<int> nums) {
    int op =0;
    for(int i=0;i<=nums.length-3;i++){
        if(nums[i]==0){
            nums[i]=1-nums[i];
            nums[i+1]=1-nums[i+1];
            nums[i+2]=1-nums[i+2];
            op++;
        }

    }
  for (int i = 0; i < nums.length; i++) {
     if (nums[i] == 0) return -1;
  }

    return op;
  }
}
